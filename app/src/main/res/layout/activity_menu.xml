<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/menu"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/background3">

    <View
        android:id="@+id/View04"
        android:layout_width="1dip"
        android:layout_height="120dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="13dp"
        android:background="#8AFFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/menu_procurar"
        app:layout_constraintStart_toEndOf="@+id/menu_registar_item"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view6"
        android:layout_width="1dip"
        android:layout_height="120dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="508dp"
        android:layout_marginEnd="13dp"
        android:background="#8AFFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/menu_problemas"
        app:layout_constraintStart_toEndOf="@+id/menu_definicoes"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view7"
        android:layout_width="1dip"
        android:layout_height="120dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="361dp"
        android:layout_marginEnd="13dp"
        android:background="#8AFFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/menu_mensagens"
        app:layout_constraintStart_toEndOf="@+id/menu_marcar_recebido"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/view8"
        android:layout_width="1dip"
        android:layout_height="120dp"
        android:layout_marginStart="13dp"
        android:layout_marginTop="214dp"
        android:layout_marginEnd="13dp"
        android:background="#8AFFFFFF"
        app:layout_constraintEnd_toStartOf="@+id/menu_items_perto"
        app:layout_constraintStart_toEndOf="@+id/menu_perfil"
        app:layout_constraintTop_toTopOf="parent" />

    <View
        android:id="@+id/View03"
        android:layout_width="120dp"
        android:layout_height="1dip"
        android:layout_marginStart="72dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="219dp"
        android:background="#8AFFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menu_registar_item" />

    <View
        android:id="@+id/view"
        android:layout_width="120dp"
        android:layout_height="1dip"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="72dp"
        android:background="#8AFFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menu_mensagens" />

    <View
        android:id="@+id/view2"
        android:layout_width="120dp"
        android:layout_height="1dip"
        android:layout_marginStart="72dp"
        android:layout_marginTop="13dp"
        android:background="#8AFFFFFF"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menu_marcar_recebido" />

    <View
        android:id="@+id/view3"
        android:layout_width="120dp"
        android:layout_height="1dip"
        android:layout_marginStart="219dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="72dp"
        android:background="#8AFFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menu_items_perto" />

    <View
        android:id="@+id/view4"
        android:layout_width="120dp"
        android:layout_height="1dip"
        android:layout_marginStart="72dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="219dp"
        android:background="#8AFFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menu_perfil" />

    <View
        android:id="@+id/view5"
        android:layout_width="120dp"
        android:layout_height="1dip"
        android:layout_marginStart="219dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="72dp"
        android:background="#8AFFFFFF"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/menu_procurar" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menu_problemas"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="72dp"
        android:background="@drawable/input_text"
        android:fontFamily="@font/ibm_plex_mono_bold"
        android:drawableTop="@drawable/problems"
        android:paddingTop="12dp"
        android:text="Reportar Problemas"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menu_procurar"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="68dp"
        android:layout_marginEnd="72dp"
        android:background="@drawable/input_text"
        android:fontFamily="@font/ibm_plex_mono_bold"
        android:drawableTop="@drawable/pesquisar"
        android:paddingTop="15dp"
        android:text="Procurar Item"
        android:onClick="MudarLayoutProcurar"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menu_perfil"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="13dp"
        android:background="@drawable/input_text"
        android:fontFamily="@font/ibm_plex_mono_bold"
        android:drawableTop="@drawable/profile"
        android:paddingTop="10dp"
        android:paddingBottom="15dp"
        android:text="Perfil"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/View03" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menu_items_perto"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="13dp"
        android:layout_marginEnd="72dp"
        android:background="@drawable/input_text"
        android:fontFamily="@font/ibm_plex_mono_bold"
        android:drawableTop="@drawable/localizacao"
        android:paddingTop="15dp"
        android:text="Items Perto de Mim"
        android:onClick="MudarLayoutPertoMim"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view5" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menu_marcar_recebido"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="13dp"
        android:background="@drawable/input_text"
        android:fontFamily="@font/ibm_plex_mono_bold"
        android:drawableTop="@drawable/checkmark"
        android:paddingTop="15dp"
        android:text="Marcar Item Recebido"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view4" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menu_mensagens"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginTop="12dp"
        android:layout_marginEnd="72dp"
        android:background="@drawable/input_text"
        android:fontFamily="@font/ibm_plex_mono_bold"
        android:drawableTop="@drawable/mensagem"
        android:paddingTop="15dp"
        android:text="Mensagens"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view3" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menu_definicoes"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="13dp"
        android:background="@drawable/input_text"
        android:fontFamily="@font/ibm_plex_mono_bold"
        android:drawableTop="@drawable/definicoes"
        android:text="Definições"
        android:paddingTop="10dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/view2" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/menu_registar_item"
        android:layout_width="120dp"
        android:layout_height="120dp"
        android:layout_marginStart="72dp"
        android:layout_marginTop="68dp"
        android:background="@drawable/input_text"
        android:fontFamily="@font/ibm_plex_mono_bold"
        android:drawableTop="@drawable/upload2"
        android:paddingTop="10dp"
        android:text="Registar Item"
        android:onClick="MudarLayoutRegistarItem"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

</androidx.constraintlayout.widget.ConstraintLayout>